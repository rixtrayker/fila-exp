{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "althinect/filament-spatie-roles-permissions": "^1.1",
        "filament/spatie-laravel-translatable-plugin": "^3.0-stable",
        "awcodes/filament-table-repeater": "*",
        "blade-ui-kit/blade-heroicons": "^2.1",
        "doctrine/dbal": "^3.6",
        "filament/filament": "^3.0-stable",
        "finller/laravel-kpi": "^1.1",
        "guzzlehttp/guzzle": "^7.2",
        "kalnoy/nestedset": "^6.0",
        "kirschbaum-development/eloquent-power-joins": "^3.0",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^2.14",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^3",
        "lucasgiovanny/filament-multiselect-two-sides": "^2.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-permission": "^5.9",
        "spatie/simple-excel": "^3.2",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "staudenmeir/laravel-cte": "^1.0",
        "staudenmeir/laravel-merged-relations": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "codeception/codeception": "^5.0",
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "^3.0-stable",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.21",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
